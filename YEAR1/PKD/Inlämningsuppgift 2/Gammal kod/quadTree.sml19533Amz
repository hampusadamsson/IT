(* REPRESENTATION CONVENTION: 
   REPRESENTATION INVARIANT:
*)
datatype quadTree = EmptyQuadTree |
Qt of rectangle * rectangle list * rectangle list *
quadTree * quadTree * quadTree * quadTree


(* emptyQtree e
